{
  "name": "jsutils",
  "version": "3.2.1",
  "description": "common javascript utils",
  "main": "build/umd/index.js",
  "module": "build/esm/index.js",
  "repository": "https://github.com/lancetipton/jsutils.git",
  "author": "lancetipton <lancetipton04@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:clean && yarn build:rollup && yarn docs",
    "build:clean": "rimraf ./build/esm && rimraf ./build/cjs",
    "build:rollup": "NODE_ENV=production rollup -c ./configs/rollup.config.js",
    "clean": "yarn cache clean && rimraf yarn.lock && rimraf node_modules && yarn install --force",
    "deploy": "yarn build && gh-pages -d docs",
    "dev": "rollup -c ./configs/rollup.config.js -w",
    "docs": "rimraf ./docs && ./node_modules/.bin/jsdoc src -d ./docs --debug --verbose --package ./package.json -t ./node_modules/docdash -R README.md && yarn docs:move",
    "docs:move": "node ./configs/moveDocs.js",
    "test": "NODE_ENV=test jest --no-cache --verbose --config=./configs/jest.config.js",
    "web": "http-server ./docs -p 8888 --cors"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-transform-object-assign": "7.2.0",
    "@babel/plugin-transform-property-literals": "7.2.0",
    "@babel/preset-env": "7.4.3",
    "@babel/runtime": "7.4.5",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "core-js": "3.1.2",
    "docdash": "1.1.1",
    "fs-extra": "8.1.0",
    "gh-pages": "2.1.1",
    "ink-docstrap": "1.3.2",
    "jest": "24.7.1",
    "jestlib": "git+https://github.com/simpleviewinc/jestlib#4d215e59992faa7111c9cd07780fcac2679359b0",
    "jsutils": "file:./",
    "jsvalidator": "^1.2.0",
    "jsdoc": "3.6.3",
    "rimraf": "3.0.0",
    "rollup": "1.20.3",
    "rollup-jest": "0.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "5.1.2"
  }
}
